		
			<!-- ============================ Page Title Start================================== -->
			<div class="page-title bg-cover" style="background:#009688;" data-overlay="5" id="cover">
				<div class="container">
					<div class="row">
						<div class="col-lg-12 col-md-12"></div>
					</div>
				</div>
			</div>
			<!-- ============================ Page Title End ================================== -->
			
			<!-- ============================ Main Section Start ================================== -->
			<section class="gray-bg pt-4">
				<div class="container-fluid">
        <div class="row m-0">
						
        <div class="col-xl-3 col-lg-4 col-md-12 col-sm-12">
          <div class="dashboard-navbar overlio-top">
            
          <div class="d-user-avater">
          <% if current_user.avatar.attached? %>
            <%= image_tag current_user.avatar, class: 'img-fluid rounded'%>  
            <% else %>
            <%= image_tag(image_path('avatar.png'), class: 'img-fluid rounded') %>
            <% end %>       
              <h4><%= current_user.firstname %> <%= current_user.lastname %></h4>
              <span><%= current_user.location %></span>
          </div>
            
            <div class="d-navigation">
              <ul id="metismenu" class="metismenu">
                 <li><%= link_to myrequests_path, class: 'nav-link' do %>
                  Manage Requests
                  <i class="fa fa-bell" aria-hidden="true"></i><span class="good"></span>
                <% end %>
                </li>
                <li>
                <%= link_to requests_path, class: 'nav-link' do %>
                  Requests
                  <i class="fa fa-bell" aria-hidden="true"></i><span class="good"></span>
                <% end %>
                </li>
                <li><%= link_to users_path, class: 'nav-link' do %>
                  Users
                  <i class="fa fa-user" aria-hidden="true"></i><span class="good"></span>
                <% end %>
                </li>
                <li><a href="reviews.html">Categories</a></li>
              </ul>
            </div>
            
          </div>
        </div>
          
						
						<!-- Item Wrap Start -->
						<div class="col-xl-9 col-lg-8 col-md-12 col-sm-12">
							<div class="row">
								<div class="col-lg-12 col-md-12 col-sm-12">
									<!-- Breadcrumbs -->
									<div class="bredcrumb_wrap">
										<nav aria-label="breadcrumb">
										  <ol class="breadcrumb">
											<li class="breadcrumb-item"><a href="#">Dasboard</a></li>
											<li class="breadcrumb-item active" aria-current="page">Users</li>
										  </ol>
										</nav>	
									</div>
								</div>
							</div>

              <div class="row">
              <% @users.each do |user| %>
								<!-- Single Item -->
								<div class="col-lg-6 col-md-6 col-sm-12">
									<div class="_freelacers_120 large shadow_0">
										<div class="_freelancers_rate"><%= user.proposals.count %> | 
                         <%= awarded_proposal.id %>
                      </div>
										<div class="_freelacers_120_thumb">
                    <% if user.avatar.attached? %>
                        <%= image_tag user.avatar, class: 'img-fluid circle' %>  
                        <% else %>
                        <%= image_tag(image_path('profile.svg'), class: 'img-fluid circle', width: '30') %>
                    <% end %>
										</div>
										<div class="_freelacers_120_caption">
											<div class="_freelan_laft"><i class="fa fa-street-view mr-1"></i><%= user.location %></div>
											<h4><%= user.firstname %> <%= user.lastname %></h4><br>
                      <h4><%= user.email %></h4>
											<span class="_freel_spec"><%= user.phone %></span><br>
                      <span class="_freel_spec"><%= user.voternumber %></span><br>
                      <span class="_freel_spec"><%= user.last_sign_in_at %></span><br>
                      <span class="_freel_spec"><%= user.current_sign_in_ip %></span><br>
                      <span class="_freel_spec"><%= user.confirmed_at %></span><br>
                      <span class="_freel_spec"><%= user.sign_in_count %></span>
											<div class="_freelan_right"><strong><i class="fa fa-star mr-1"></i>4.9</strong></div>
										</div>
										<div class="_freelacers_120_body">
											<div class="_free0o9">
												<ul>
                        <% if user.skills.any? %>
                        <i class="fa fa-handshake-o"></i> <%= render user.skills %>
                        <% end %>
												</ul>
											</div>
                      <div class="_free0o9">
												<ul>
                        <li><% user.roles.each do |role| %>
                          <span><%= link_to role.name %></span>
                          <% end %>
                        </li>
												</ul>
											</div>
											<div class="_freelacers_121_foot">
                         <%= link_to user.lastname, user %>
											</div>
										</div>
									</div>
								</div>
								<% end %>
							</div>
              </div>
            </div>

                    
</div>
</div>
</section>




